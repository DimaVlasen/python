import sympy as sp 
from math import factorial 
def taylor(x): 
    y = 0 
    d1 = sp.diff(f, x)  
    d2 = sp.diff(d1, x)  
    d3 = sp.diff(d2, x)   
    print('d1=',d1,'d2=',d2,'d3=',d3) 
    y += f + d1*e + d2*sin+d3**(5*x) 
    print ('y = ', y) 
    
    return y 
x = sp.symbols('x') 
f = sp.e**sin*(5*x) 
taylor_x = taylor(x) 

sp.plot(taylor_x, f, (x, -1, 1), label='Taylor') 
